def quick_job():
    with open("wallets_and_ammounts.txt", "r") as f:
        wallets_and_ammounts_list = [l.strip() for l in f]
    wallets = [l.split(" ")[0] for l in wallets_and_ammounts_list]
    cheched_wallets = []
    multidimensional_list = []
    for wallet, wallet_and_ammount in zip(wallets, wallets_and_ammounts_list):
        one_wallet_all_ammounts_list = []
        if wallet in cheched_wallets:
            continue
        for i in range(len(wallets)):
            if wallet == wallets[i]:
                one_wallet_all_ammounts_list.append(wallets_and_ammounts_list[i])
        cheched_wallets.append(wallet)
        multidimensional_list.append(one_wallet_all_ammounts_list)
    multidimensional_list = sorted(multidimensional_list, key=len, reverse=True)
    multidimensional_list = [item for sublist in multidimensional_list for item in sublist]

    wallets_and_ammounts_list = multidimensional_list
    wallets = [l.split(" ")[0] for l in wallets_and_ammounts_list]


    cheched_wallets = []
    final_dict = {}
    add_to_the_end = []
    for i, (wallet, wallet_and_ammount) in enumerate(zip(wallets, wallets_and_ammounts_list)):
        if wallets[i] not in cheched_wallets:
            if wallets.count(wallets[i]) > 8:
                this_wallet_ammounts_list = [l for l in wallets_and_ammounts_list if wallets[i] == l.split(" ")[0]]
                first_part, second_part = this_wallet_ammounts_list[:8], this_wallet_ammounts_list[8:]
                # print(first_part, second_part)
                for k in range(0, 8):
                    final_dict[k*43 + len(cheched_wallets)] = first_part[k]
                add_to_the_end.extend(second_part)
            else:
                this_wallet_ammounts_list = [l for l in wallets_and_ammounts_list if wallets[i] == l.split(" ")[0]]
                # print(this_wallet_ammounts_list)
                for k in range(0, wallets.count(wallets[i])):
                    place_to_put = 0
                    while (place_to_put + (k*43 + len(cheched_wallets))) in final_dict:
                        place_to_put +=1
                    final_dict[place_to_put + k*43 + len(cheched_wallets)] = this_wallet_ammounts_list[k]

            cheched_wallets.append(wallets[i])
    final_list = []
    for k, v in sorted(final_dict.items()):
        final_list.append(v)
    final_list.extend(add_to_the_end)
    pprint(final_list)
    
